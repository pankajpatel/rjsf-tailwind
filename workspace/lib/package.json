{
  "name": "@pankajpatel/rjsf-tailwind",
  "version": "0.0.1",
  "description": "TypeScript React Library",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./rjsf": {
      "types": "./dist/rjsf/index.d.ts",
      "import": "./dist/rjsf/index.js",
      "require": "./dist/rjsf/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:css && tsc --project tsconfig.build.json",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
    "dev": "yarn run dev:ts & yarn run dev:css",
    "dev:ts": "tsc --watch",
    "dev:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwind"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@rjsf/core": "^5.23.1",
    "@rjsf/utils": "^5.23.1",
    "@rjsf/validator-ajv8": "*",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@rjsf/core": "^5.23.1",
    "@rjsf/utils": "^5.23.1",
    "@rjsf/validator-ajv8": "^5.23.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "semantic-release": "^23.0.2",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "clsx": "^2.1.1"
  }
}
